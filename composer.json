{
  "name": "andyli0123/flarum-lang",
  "description": "Traditional Chinese pack for Flarum.",
  "keywords": [
    "flarum",
    "locale",
    "extension",
    "Chinsese",
    "中文",
    "繁體中文"
  ],
  "type": "flarum-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "Andy Li",
      "email": "andyli0123boy@gmail.com",
      "homepage": "https://github.com/andyli0123"
    }
  ],
  "support": {
    "source": "https://github.com/andyli0123/flarum-lang",
    "issues": "https://github.com/andyli0123/flarum-lang/issues",
    "forum": "https://github.com/andyli0123/flarum-lang"
  },
  "require": {
    "flarum/core": "^1.0.0"
  },
  "extra": {
    "flarum-extension": {
      "title": "繁體中文",
      "category": "language",
      "icon": {
        "image": "icon.svg",
        "backgroundColor": "#fff",
        "backgroundSize": "cover",
        "backgroundPosition": "center"
      }
    },
    "flarum-locale": {
      "code": "zh-TW",
      "title": "繁體中文"
    }
  }
}
